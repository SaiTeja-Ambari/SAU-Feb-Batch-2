package javacore;
import java.util.List;  

import javax.xml.bind.annotation.*;  
@XmlRootElement  
public class Studs {  
private List<Stud> student;  

public Studs() {}  
public Studs(List<Stud> stu) {  
    super(); 
    this.stud = stud;  
}  
  
@XmlElement  
public List<Student> getStudent() {  
    return student;  
}  
public void setStudent(List<Student> student) {  
    this.student = student;  
}  
}  

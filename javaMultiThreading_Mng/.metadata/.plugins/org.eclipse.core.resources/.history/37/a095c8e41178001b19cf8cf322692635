import java.io.BufferedReader;
import java.io.FileReader;


public class CountWithThreads implements Runnable{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}
	public void main(String ab[]){
    int n = 4; 
    for (int i = 0; i < n; i++) {
        Thread object
            = new Thread(new CountWithThreads());
        object.start();
    }
			
	}
}
